    <script>

        function businessEnquiry()
        {

           var name = $("#name").val(); var email = $("#email").val(); var message = $("#message").val();
           var telephone = $("#mobile").val();
           if (name == null) {
               alert("Name cannot be empty, Please try again");
           }
           else if (email == null) {
               alert("Email cannot be empty, Please try again");
           }
           else if (telephone == null) {
               alert("Mobile number cannot be empty, Please try again");
           }
           else if (message == null) {
               alert("Message cannot be empty, Please try again");
           }
           else {
            ga('send', 'event', email + telephone, 'ENQUIRY', 'vanajahospital');
               var formdata = JSON.stringify({
                   "name": name,
                   "from": email   + ":\n"+ telephone,
                   "message": message,
                   "username": "vanajahospital"
               });
               $.ajax({
                   url: 'http://emartshop.in/Home/SubmitEnquiry',
                   dataType: 'json',
                   data: formdata,
                   contentType: 'application/json',
                   type: 'POST',
                   success: function (response) {

                   },
                   error: function (response) {
                       alert("Thanks for contacting us, we will get back to you shortly");
                   }
               });
           }
        }


    </script>

<script>

    function businessEnquiry1()
    {

       var fname = $("#fname").val(); var email = $("#emailid").val(); var message = $("#messagedetails").val();
       var telephone = $("#mobilenumber").val(); var lname = $("#lname").val();
       if (fname == null) {
           alert("Name cannot be empty, Please try again");
       }
       else if (email == null) {
           alert("Email cannot be empty, Please try again");
       }
       else if (telephone == null) {
           alert("Mobile number cannot be empty, Please try again");
       }
       else if (message == null) {
           alert("Message cannot be empty, Please try again");
       }
       else {
        ga('send', 'event', email + telephone, 'ENQUIRY', 'vanajahospital');
           var formdata = JSON.stringify({
               "name": fname + " " + lname,
               "from": email   + ":\n"+ telephone,
               "message": message,
               "username": "vanajahospital"
           });
           $.ajax({
               url: 'http://emartshop.in/Home/SubmitEnquiry',
               dataType: 'json',
               data: formdata,
               contentType: 'application/json',
               type: 'POST',
               success: function (response) {

               },
               error: function (response) {
                   alert("Thanks for contacting us, we will get back to you shortly");
               }
           });
       }
    }


</script>

</body>

</html>
